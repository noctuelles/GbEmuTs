{
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "jsxBracketSameLine": false,
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "parser": "markdown"
        }
      }
    ]
  }